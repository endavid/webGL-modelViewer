{
  "extends": "airbnb/base",
  "env": {
    "browser": true
  },
  "rules": {
    "arrow-body-style": ["error", "as-needed"],
    "linebreak-style": 0,
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "no-console": 0,
    "import/extensions": 0,
    "lines-between-class-members": 0
  }
}
