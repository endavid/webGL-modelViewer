{
  "name": "EnDavid ModelViewer",
  "version": "2.0",
  "icons": {
    "24": "resources/logo-24.png",
    "128": "resources/logo-128.png"
  },
  "app": {
    "launch": {
      "local_path": "modelViewer.html"
    }
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "file://*"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://code.jquery.com https://*.jquery.com; object-src 'self'"
}
